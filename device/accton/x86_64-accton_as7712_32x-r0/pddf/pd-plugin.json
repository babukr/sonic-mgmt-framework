{
    "PSU":
    {
        "psu_present": 
        {
            "valmap": { "1":true, "0":false }
        },

        "psu_power_good": 
        {
            "valmap": { "1": true, "0":false }
        },

        "psu_fan_dir":
        {
            "valmap": { "F2B":"INTAKE", "B2F":"EXHAUST" }
        },

        "PSU_FAN_MAX_SPEED":"18000"
    },

    "FAN":
    {
        "direction":
        {
            "valmap": {"1":"INTAKE", "0":"EXHAUST"}
        },

        "present":
        {
            "valmap": {"1":true, "0":false}
        },
        
        "duty_cycle_to_pwm": "lambda dc: ((dc*100)/625 - 1)",

        "pwm_to_duty_cycle": "lambda pwm: (((pwm+1)*625)/100)"
    }

}
