{
    "XCVR":
    {
        "xcvr_present": 
        {
           "i2c": 
           {
               "valmap-SFP28": {"1":true, "0":false },
               "valmap-QSFP28": {"1":false, "0":true}
           }
        }
    },

    "PSU":
    {
        "psu_present": 
        {
            "i2c": 
            {
                "valmap": { "1":true, "0":false }
            },
            "bmc": 
            {
                "valmap": { "0":true, "1":false }
            }
        },

        "psu_power_good": 
        {
            "i2c":
            {
                "valmap": { "1": true, "0":false }
            },
            "bmc":
            {
                "valmap": { "ok": true, "ns":false }
            }
        },

        "psu_fan_dir":
        {
            "i2c":
            {
                "valmap": { "F2B":"INTAKE", "B2F":"EXHAUST" }
            },
            "bmc":
            {
                "valmap": { "1":"EXHAUST", "2":"INTAKE" }
            }
        },

        "PSU_FAN_MAX_SPEED":"23000"
    },

    "FAN":
    {
        "direction":
        {
            "i2c":
            {
                "valmap": {"1":"INTAKE", "0":"EXHAUST"}
            },
            "bmc":
            {
                "valmap": {"1":"EXHAUST", "2":"INTAKE"}
            }
        },

        "present":
        {
            "i2c":
            {
               "valmap": {"1":true, "0":false}
            },
            "bmc":
            {
               "valmap": {"0":true, "1":false}
            }
        },
        
        "pwm_to_duty_cycle": "lambda pwm: (((pwm+1)*625)/100)"

    }

}
