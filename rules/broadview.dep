
SPATH       := $($(SONIC_BROADVIEW)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/broadview.mk rules/broadview.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(SONIC_BROADVIEW)_CACHE_MODE  := GIT_CONTENT_SHA 
$(SONIC_BROADVIEW)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SONIC_BROADVIEW)_DEP_FILES   := $(DEP_FILES)
$(SONIC_BROADVIEW)_SMDEP_FILES := $(SMDEP_FILES)
$(SONIC_BROADVIEW)_SMDEP_PATHS := $(SPATH)

