
SPATH       := $($(SONIC_PDE_TESTS)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/sonic-pde-tests.mk rules/sonic-pde-tests.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(SONIC_PDE_TESTS)_CACHE_MODE  := GIT_CONTENT_SHA 
$(SONIC_PDE_TESTS)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SONIC_PDE_TESTS)_DEP_FILES   := $(DEP_FILES)
$(SONIC_PDE_TESTS)_SMDEP_FILES := $(SMDEP_FILES)
$(SONIC_PDE_TESTS)_SMDEP_PATHS := $(SPATH)

