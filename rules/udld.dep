
SPATH       := $($(UDLD)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/udld.mk rules/udld.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(UDLD)_CACHE_MODE  := GIT_CONTENT_SHA 
$(UDLD)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(UDLD)_DEP_FILES   := $(DEP_FILES)
$(UDLD)_SMDEP_FILES := $(SMDEP_FILES)
$(UDLD)_SMDEP_PATHS := $(SPATH)

