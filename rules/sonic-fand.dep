
SPATH       := $($(SONIC_FAND)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/sonic-fand.mk rules/sonic-fand.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(SONIC_FAND)_CACHE_MODE  := GIT_CONTENT_SHA 
$(SONIC_FAND)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SONIC_FAND)_DEP_FILES   := $(DEP_FILES)
$(SONIC_FAND)_SMDEP_FILES := $(SMDEP_FILES)
$(SONIC_FAND)_SMDEP_PATHS := $(SPATH)

