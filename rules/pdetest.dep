
SPATH       := $($(LIBPDETEST)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/pdetest.mk rules/pdetest.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(LIBPDETEST)_CACHE_MODE  := GIT_CONTENT_SHA 
$(LIBPDETEST)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(LIBPDETEST)_DEP_FILES   := $(DEP_FILES)
$(LIBPDETEST)_SMDEP_FILES := $(SMDEP_FILES)
$(LIBPDETEST)_SMDEP_PATHS := $(SPATH)

